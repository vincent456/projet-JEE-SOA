function logerror(jqXHR,textStatus,errorThrown){
	console.log(textStatus);
	console.log(errorThrown);
}

function submit() {
	server = $("#server")[0].value;
	content = $("#input")[0].value;
	
	date = new Date();
	//timestamp = date.getTime();
	timestamp = date;
	data ={message:content,timestamp:timestamp};
	
	$.ajax({
		type:"POST",
		crossDomain:true,
		url:server+"/post",
		data:data,
		dataType:"json",
		success:null,
		error:logerror
	});
	
}

